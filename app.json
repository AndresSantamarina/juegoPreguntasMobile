{
  "expo": {
    "name": "Sugar Rush Quiz",
    "slug": "preguntasMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-question.png",
    "scheme": "preguntasmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-question.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.Andres912.sugarrushquiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/icon-question.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/splash-screen-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#192f6a"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./src/assets/splash-screen-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#192f6a"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92d19259-6849-450b-b7c2-da613969e198"
      }

    }
  }
}
