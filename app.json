{
  "expo": {
    "name": "Sugar Rush Quiz",
    "slug": "preguntasMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-question.png",
    "scheme": "preguntasmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Andres912.sugarrushquiz",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-question.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "edgeToEdgeEnabled": true,
      "package": "com.Andres912.sugarrushquiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/icon-question.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/splash-screen-dark.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#192f6a"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./src/assets/splash-screen-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#192f6a"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92d19259-6849-450b-b7c2-da613969e198"
      },
      "EXPO_PUBLIC_API_URI": "https://backend-juego-preguntas.vercel.app/api",
      "EXPO_PUBLIC_API_PREGUNTAS": "https://backend-juego-preguntas.vercel.app/api/preguntas"
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-awesome-alerts", "react-native-vector-icons"]
      }
    }
  }
}
